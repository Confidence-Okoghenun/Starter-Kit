{
 "name": "starter_kit",
 "version": "1.0.0",
 "description": "Starter Kit",
 "scripts": {
  "prestart": "node_modules/.bin/babel-node buildScripts/startMessage.js",
  "start": "node_modules/.bin/run-p  serve lint:watch  test:watch",
  "serve": "node_modules/.bin/babel-node buildScripts/srcServer.js",
  "prepush": "git push",
  "push": "node_modules/.bin/surge",
  "lint": "node_modules/.bin/esw webpack.config.* src buildScript --color",
  "lint:watch": "yarn lint --watch",
  "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
  "test:watch": "yarn test --watch",
  "generate-mock-data": "node_modules/.bin/babel-node buildScripts/generateMockData",
  "prestart-mockapi": "yarn generate-mock-data",
  "start-mockapi": "node_modules/.bin/json-server --watch src/api/db.json --port 4041",
  "clean-dist": "node_modules/.bin/rimraf ./dist && mkdir dist",
  "prebuild": "node_modules/.bin/npm-run-all clean-dist test lint",
  "build": "node_modules/.bin/babel-node buildScripts/build.js",
  "postbuild": "node_modules/.bin/babel-node buildScripts/distServer.js"
 },
 "author": "Confidence Okoghenun",
 "license": "MIT",
 "dependencies": {
  "whatwg-fetch": "1.0.0"
 },
 "devDependencies": {
  "babel-cli": "6.16.0",
  "babel-core": "6.17.0",
  "babel-loader": "6.2.5",
  "babel-preset-latest": "6.16.0",
  "babel-register": "6.16.3",
  "chai": "3.5.0",
  "chalk": "1.1.3",
  "cheerio": "0.22.0",
  "compression": "1.6.2",
  "cross-env": "3.1.3",
  "css-loader": "0.25.0",
  "eslint": "3.8.1",
  "eslint-plugin-import": "2.0.1",
  "eslint-watch": "2.1.14",
  "express": "4.14.0",
  "extract-text-webpack-plugin": "1.0.1",
  "html-webpack-plugin": "2.22.0",
  "jsdom": "9.8.0",
  "json-schema-faker": "0.3.6",
  "json-server": "0.8.22",
  "localtunnel": "1.8.1",
  "mocha": "3.1.2",
  "nock": "8.1.0",
  "npm-run-all": "3.1.1",
  "numeral": "1.5.3",
  "open": "0.0.5",
  "rimraf": "2.5.4",
  "style-loader": "0.13.1",
  "surge": "0.20.4",
  "webpack": "1.13.2",
  "webpack-dev-middleware": "1.8.4",
  "webpack-hot-middleware": "2.13.0",
  "webpack-md5-hash": "0.0.5"
 }
}
